let _ =
  ( nil |>
    dig (Rnat.of_int 1) |>
    dup |> car |> dip (Rnat.of_int 1) cdr |>
    drop (Rnat.of_int 1) |>
    push Address |>
    contract |>
    if_none (push MString |> failwith) skip |>
    push Address |>
    contract |>
    if_none (push MString |> failwith) skip |>
    push Mutez |>
    push Unit |>
    transfer_tokens |>
    cons |>
    dip (Rnat.of_int 1) (dig (Rnat.of_int 3) |> drop (Rnat.of_int 1)) |>
    dug (Rnat.of_int 3) |>
    dig (Rnat.of_int 3) |>
    dup |>
    dug (Rnat.of_int 4) |>
    dig (Rnat.of_int 1) |>
    dup |>
    dug (Rnat.of_int 2) |>
    push Mutez |>
    push Unit |>
    transfer_tokens |>
    cons |>
    dip (Rnat.of_int 1) (dig (Rnat.of_int 3) |> drop (Rnat.of_int 1)) |>
    dug (Rnat.of_int 3) |>
    drop (Rnat.of_int 2) |>
    dig (Rnat.of_int 1) |>
    pair
  )
  (Pair (Unit, Unit) :: [])
